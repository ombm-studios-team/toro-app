{
  "name": "toro-app",
  "version": "0.0.0",
  "scripts": {
    "build-storybook": "ng run toro-app:build-storybook",
    "build": "ng build",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "ng": "ng",
    "posttest": "npm run format",
    "prepare": "husky install",
    "pretest": "npm run lint",
    "start": "ng serve",
    "storybook": "ng run toro-app:storybook",
    "test": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.5",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.5",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "15.0.0",
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.0",
    "@briebug/jest-schematic": "^6.0.0",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-essentials": "^7.1.0-alpha.0",
    "@storybook/addon-interactions": "^7.1.0-alpha.0",
    "@storybook/addon-links": "^7.1.0-alpha.0",
    "@storybook/angular": "^7.1.0-alpha.0",
    "@storybook/blocks": "^7.1.0-alpha.0",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/jest": "29.5.0",
    "husky": "^8.0.0",
    "jest": "28.1.3",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.1.0-alpha.0",
    "typescript": "~4.9.4"
  },
  "engines": {
    "node": ">=10"
  }
}
